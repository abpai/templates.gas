{
  "name": "gas-template",
  "version": "1.0.0",
  "description": "A template for Google Apps Script projects.",
  "main": "index.js",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "push:watch": "clasp push --watch",
    "dev:sync": "concurrently -k \"pnpm run dev\" \"pnpm run push:watch\"",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:staged": "lint-staged",
    "test": "vitest run",
    "build": "vite build --mode production",
    "build:test": "npm run env:test && vite build --mode test",
    "build:prod": "npm run env:prod && vite build --mode production",
    "push": "npm run build && clasp push",
    "deploy": "clasp deploy --description \"CI auto-deploy\"",
    "prepare": "husky || true",
    "test:watch": "vitest",
    "typecheck": "tsc --noEmit",
    "env:switch": "node scripts/env-switch.js switch",
    "env:current": "node scripts/env-switch.js current",
    "env:test": "node scripts/env-switch.js switch test",
    "env:prod": "node scripts/env-switch.js switch production",
    "push:test": "npm run build:test && clasp push",
    "push:prod": "npm run build:prod && clasp push",
    "deploy:test": "npm run push:test && clasp deploy --description \"Test deployment\"",
    "deploy:prod": "npm run push:prod && clasp deploy --description \"Production deployment\""
  },
  "type": "module",
  "keywords": [
    "google-apps-script",
    "gas",
    "template"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/google-apps-script": "^1.0.99",
    "@types/node": "^24.0.3",
    "@typescript-eslint/eslint-plugin": "8.34.1",
    "@typescript-eslint/parser": "8.34.1",
    "commander": "^14.0.0",
    "concurrently": "^9.2.0",
    "csv-parse": "^5.6.0",
    "csv-stringify": "^6.5.2",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-googleappsscript": "^1.0.5",
    "eslint-plugin-prettier": "^5.5.0",
    "gas-mock-globals": "^2.2.0",
    "gemini-zod": "^0.1.2",
    "globals": "16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "openai": "^5.8.3",
    "prettier": "^3.5.3",
    "rollup-plugin-google-apps-script": "^2.0.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.34.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4",
    "zod": "^3.25.76"
  },
  "lint-staged": {
    "*.{js,ts}": "eslint --fix"
  },
  "packageManager": "pnpm@10.12.4+sha512.5ea8b0deed94ed68691c9bad4c955492705c5eeb8a87ef86bc62c74a26b037b08ff9570f108b2e4dbd1dd1a9186fea925e527f141c648e85af45631074680184"
}